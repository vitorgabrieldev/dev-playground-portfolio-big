{
	"private": true,
	"scripts": {
		"homolog": "yarn run homologation",
		"homologation": "cross-env NODE_ENV=homologation NODE_OPTIONS=--openssl-legacy-provider webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",

		"dev": "yarn run development",
		"development": "cross-env NODE_ENV=development NODE_OPTIONS=--openssl-legacy-provider webpack --progress --config=node_modules/laravel-mix/setup/webpack.config.js",

		"watch": "yarn run development -- --watch",
		"watch-poll": "yarn run watch -- --watch-poll",

		"hot": "cross-env NODE_ENV=development NODE_OPTIONS=--openssl-legacy-provider webpack-dev-server --inline --hot --disable-host-check --config=node_modules/laravel-mix/setup/webpack.config.js",

		"prod": "yarn run production",
		"production": "cross-env NODE_ENV=production NODE_OPTIONS=--openssl-legacy-provider webpack --no-progress --config=node_modules/laravel-mix/setup/webpack.config.js"
	},
